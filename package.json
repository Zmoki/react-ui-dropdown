{
  "name": "sexy-dropdown",
  "config": {
    "name": "sexy-dropdown",
    "library": "SexyDropdown"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "version": "0.1.0",
  "description": "Sexy Dropdown â€” dropdown like in vk.com.",
  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel-core/register",
    "test:watch": "npm run test -- --watch",
    "check": "npm run lint && npm run test",
    "build:lib": "babel src --out-dir lib",
    "build:dist": "webpack src/index.js --config webpack.config.development.js",
    "build:dist:min": "webpack src/index.js --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:dist && npm run build:dist:min",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zmoki/sexy-dropdown.git"
  },
  "author": "Zarema Khalilova",
  "license": "MIT",
  "homepage": "https://github.com/Zmoki/sexy-dropdown",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "eslint": "^1.10.1",
    "expect": "^1.13.0",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.4",
    "webpack": "^1.12.9"
  },
  "npmName": "sexy-dropdown",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "dependencies": {
    "convert-layout": "^0.4.1"
  }
}
